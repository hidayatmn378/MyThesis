keywords = {4D flow MRI,Aortic stenosis,Pressure drop,Velocity vector ultrasound},
mendeley-groups = {MyThesis},
month = {dec},
publisher = {Elsevier},
title = {{Evaluation of aortic stenosis: From Bernoulli and Doppler to Navier-Stokes}},
