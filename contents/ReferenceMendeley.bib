journal = {Ocean Modelling},
keywords = {Arctic Ocean,Ice breakage,Ocean modeling,Positive feedback,Wave attenuation},
month = {jan},
pages = {101532},
publisher = {Elsevier Ltd.},
title = {{Applications of an unstructured grid surface wave model (FVCOM-SWAVE) to the Arctic Ocean: The interaction between ocean waves and sea ice}},
url = {https://linkinghub.elsevier.com/retrieve/pii/S1463500318302488},
volume = {145},
